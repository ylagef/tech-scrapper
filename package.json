{
  "name": "tech-scrapper",
  "version": "1.0.0",
  "description": "Tech scrapper",
  "main": "index.js",
  "scripts": {
    "start": "forever start --watchDirectory=./src ./src/forever/config.json",
    "stop-all": "forever stopall",
    "stop": "forever stop tech-scrapper",
    "forever-logs": "forever logs 0 -f",
    "logs": "tail -f log/out.log",
    "errors": "tail -f log/err.log",
    "restart": "npm run stop && npm run start",
    "start-logs": "npm run start && npm run logs",
    "node-start": "npx playwright install && node src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ylagef/tech-scrapper.git"
  },
  "author": "Yeray Lage",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ylagef/tech-scrapper/issues"
  },
  "homepage": "https://github.com/ylagef/tech-scrapper#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "forever": "^4.0.1",
    "google-spreadsheet": "^3.2.0",
    "md5-nodejs": "^1.0.3",
    "node-color-log": "^7.0.0",
    "node-telegram-bot-api": "^0.54.0",
    "playwright": "^1.21.0"
  },
  "devDependencies": {
    "standard": "^16.0.4"
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
