  "bugs": {
    "url": "https://github.com/ylagef/tech-scrapper/issues"
  },
  "homepage": "https://github.com/ylagef/tech-scrapper#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "forever": "^4.0.1",
    "google-spreadsheet": "^3.2.0",
    "md5-nodejs": "^1.0.3",
    "node-color-log": "^7.0.0",
    "node-telegram-bot-api": "^0.53.0",
    "playwright": "^1.16.3"
  },
  "devDependencies": {
    "standard": "^16.0.4"
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
