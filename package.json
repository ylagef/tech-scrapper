{
  "name": "tech-scrapper",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "forever start -a -o ./forever/out.log -e ./forever/err.log --minUptime 5000 --spinSleepTime 5000 index.js",
    "stop": "forever stopall",

    "logs": "tail -f forever/out.log",
    "errors": "tail -f forever/err.log",

    "restart": "npm run stop && npm run start",
    "start-logs":"npm run start && npm run logs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ylagef/tech-scrapper.git"
  },
